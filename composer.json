{
    "name": "opensourcerefinery/yaml2pimple",
    "keywords": ["pimple", "dependency injection", "container"],
    "description": "Build a Pimple/Container from a config file",
    "license":"MIT",
    "authors": [
        {
            "name": "Gonzalo Ayuso",
            "email": "gonzalo123@gmail.com"
        },
        {
            "name": "Michael Koert",
            "email": "mkoert@bluebikeproductions.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "OpenSourceRefinery\\Yaml2Pimple\\": "src/"
        }
    },
    "require": {
        "pimple/pimple": "~3.0",
        "symfony/config": "~2.8",
        "symfony/yaml": "~2.8"
    },
    "config": {
        "bin-dir": "bin/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
